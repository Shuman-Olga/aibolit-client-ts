{
  "main": "src/index.tsx",
  "name": "aibolit-client-ts",
  "config": {
    "dev": "--config __webpack__/dev.config.js",
    "prod": "--config __webpack__/prod.config.js"
  },
  "version": "0.1.0",
  "private": true,
  "description": "Site Aibolit",
  "author": "Shuman Olga",
  "scripts": {
    "webpack-config-dev": "nodemon --watch \"./__webpack__/*\" --exec npm run start-dev",
    "webpack-config-prod": "nodemon --watch \"./__webpack__/*\" --exec npm run start-prod",
    "start-dev": "cross-env-shell webpack serve ${npm_package_config_dev}",
    "start-prod": "cross-env-shell webpack serve ${npm_package_config_prod}",
    "build-dev": "cross-env-shell webpack ${npm_package_config_dev}",
    "build-prod": "cross-env-shell webpack ${npm_package_config_prod} --stats-children && react-snap",
    "clean": "rd /s /q build",
    "lint": "eslint src --ext .js --ext .ts",
    "lint-fix": "eslint src --ext .js --ext .ts --fix",
    "test": "cross-env jest --config __jest__/jest.config.js",
    "test-watch": "jest --watch --config __jest__/jest.config.js",
    "test-coverage": "jest --coverage --config __jest__/jest.config.js"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "@hookform/resolvers": "^3.3.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.91",
    "@types/react": "^18.2.71",
    "@types/react-dom": "^18.2.22",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "material-icons": "^1.13.12",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.51.2",
    "react-pdf": "^7.7.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "use-react-router-breadcrumbs": "^4.0.1",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.4"
  },
  "reactSnap": {
    "inlineCss": true,
    "skipThirdPartyRequests": true,
    "puppeteer": {
      "timeout": 100000
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/react-helmet": "^6.1.11",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "filemanager-webpack-plugin": "^8.0.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "image-minimizer-webpack-plugin": "^4.0.0",
    "image-webpack-loader": "^8.1.0",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "jest": "^29.1.2",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.8.1",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.0",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.4",
    "redux-logger": "^3.0.6",
    "sass": "^1.72.0",
    "sass-loader": "^14.1.1",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5",
    "typescript-plugin-css-modules": "^5.1.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
